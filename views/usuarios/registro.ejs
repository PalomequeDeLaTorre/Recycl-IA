<%- include ("../templates/encabezado") %>
<%- include ("../templates/menu")%>

<div class="container mt-5">
  <form action="/register" method="post" enctype="multipart/form-data">
      <div class="card">
          <div class="card-header">
              <h1>Registro</h1>
          </div>
          <div class="card-body">
              <input class="form-control mb-3" type="text" name="usuario" id="usuario" placeholder="Usuario" autofocus>
              <input class="form-control mb-3" type="text" name="telefono" id="telefono" placeholder="Telefono">
              <input class="form-control mb-3" type="password" name="password" id="password" placeholder="Password">
          </div>
          <div class="card-footer">
              <button class="form-control btn btn-primary" type="submit">Registrarse</button>
          </div>
      </div>
  </form>
</div>

<%- include ("../templates/pie") %>
